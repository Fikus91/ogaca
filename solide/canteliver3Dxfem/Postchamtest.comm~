MODEMEV=AFFE_MODELE(MAILLAGE=MAIL,AFFE=_F(GROUP_MA=('Group_1v','Group_2v','Group_3v','VOLv'),PHENOMENE='MECANIQUE',MODELISATION='3D',),);



