#DETRUIRE(INFO=1,CONCEPT=_F(NOM=(CHLVL,),),);
#CHLVL=CREA_CHAMP( OPERATION='ASSE', TYPE_CHAM='NOEU_NEUT_R',MODELE=MODEMEC, 
